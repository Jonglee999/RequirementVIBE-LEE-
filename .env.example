# Centralized LLM API (required)
# Replace with your actual centralized API key
CENTRALIZED_LLM_API_KEY=your_centralized_llm_api_key_here

# Unstructured Serverless API (required for document processing)
UNSTRUCTURED_API_KEY=your_unstructured_api_key_here
# Optional custom endpoint (default shown)
UNSTRUCTURED_API_URL=https://api.unstructured.io/general/v0/general

# Email service (optional - only if you enable email verification flows)
# RESEND_API_KEY=your_resend_api_key_here

# LangSmith monitoring configuration
# The provided API key is included for convenience.
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_PROJECT=ReqVibe-Monitoring
# Enable in-app tracing hooks
LANGSMITH_ENABLED=true
